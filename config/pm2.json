[
  {
    "name": "jwt-sales-finder",
    "cwd": "/home/ubuntu/apps/jwt-sales-finder",
    "script": "bin/www",
    "max_memory_restart": "350M",
    "watch": false,
    "instances": 0,
    "node_args": "--harmony_destructuring",
    "env": {
      "PORT": 4002,
      "APP_NAME": "sales-finder",
      "NODE_ENV": "production",
      "UPLOAD_FOLDER": "/home/ubuntu/apps/jwt-sales-finder/server/uploads",
      "DATABASE": "mysql://root@localhost:3306/jwt-sales-finder",
      "MIGRATE": "safe",
      "JWT_ISSUER": "telus-jwt",
      "JWT_SECRET": "[should-match-the-ACCESS-secret-on-the-auth-manager-server]"
    }
  }
]
